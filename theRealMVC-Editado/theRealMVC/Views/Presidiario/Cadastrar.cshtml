@model Presidiario

@{
    ViewData["Title"] = "Cadastrar";
}

<h2>Cadastrar</h2>

<form asp-action="cadastrar">
    <div class="form-group">
        <label asp-for="Nome">Nome: </label>
        <input asp-for="Nome" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="dataEntrada">Data Entrada: </label>
        <input asp-for="dataEntrada" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="dataSaida">Data Saida: </label>
        <input asp-for="dataSaida" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="nivelEscolaridade">Nivel Escolaridade: </label>
        <select asp-items="@Html.GetEnumSelectList<NivelEscolaridade>()"><option>Selecionar</option></select>

    </div>
    <div class="form-group">
        <label asp-for="Cela">Cela: </label>
        <select asp-for="CelaId" class="form-control" asp-items="@ViewBag.celas"><option>Selecione</option></select>
    </div>

    <input type="submit" value="Cadastrar" class="btn btn-success" />
</form>